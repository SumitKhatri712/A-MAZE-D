config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="A-MAZE-D"
config/description="A-MAZE-D"
run/main_scene="res://Maps/Map Scenes/TitleScreen.tscn"
config/icon="res://UI/Images/Golem_Icon.png"
config/windows_native_icon="res://UI/Images/Golem_Icon.ico"

[autoload]

SceneChanger="*res://Scenes/SceneChanger.tscn"
Globals="*res://Scripts/Globals.gd"
BgMusic="*res://Scenes/BG_music.tscn"
AudioManager="*res://Scripts/audioManager.gd"
SaveNLoad="*res://Scripts/Save_N_load.gd"

[debug]

settings/stdout/verbose_stdout=true

[display]

window/size/width=320
window/size/height=180
window/size/test_width=1024
window/size/test_height=580
window/stretch/mode="2d"
window/stretch/aspect="keep_height"

[editor_plugins]

enabled=PoolStringArray( "vnen.tiled_importer" )

[importer_defaults]

vnen.tiled_tileset_importer={
"apply_offset": false,
"custom_properties": true,
"embed_internal_images": true,
"image_flags": 0,
